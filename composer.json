{
    "name": "datana-gmbh/logz-io-handler",
    "type": "library",
    "description": "A pre configured handler for logz.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Oskar Stark",
            "email": "oskarstark@googlemail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "inpsyde/logzio-monolog": "^1.0",
        "monolog/monolog": "^2.0",
        "oskarstark/trimmed-non-empty-string": "^0.1.0 || ^0.2.0 || ^1.0",
        "webmozart/assert": "^1.9"
    },
    "require-dev": {
        "ergebnis/test-util": "^1.4",
        "phpunit/phpunit": "^9.0",
        "symfony/string": "^5.1"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Datana\\LogzIo\\Handler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Datana\\LogzIo\\Handler\\Tests\\": "tests/"
        }
    }
}
